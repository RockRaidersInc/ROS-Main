<?xml version="1.0"?>
<launch>
    <node name="map_server" pkg="map_server" type="map_server" args="$(find autonomous_nav)/maps/empty.yaml"/>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find launch_files)/params/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
        <rosparam file="$(find launch_files)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find launch_files)/params/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find launch_files)/params/global_costmap_params.yaml" command="load" /> 
        <rosparam file="$(find launch_files)/params/base_local_planner_params.yaml" command="load" />

        <rosparam file="$(find launch_files)/params/move_base_params.yaml" command="load" />

        <remap from="odom" to="/odometry/filtered" />
        <remap from="cmd_vel" to="/cmd_vel" />
        <remap from="/map" to="/map" /> 
    </node>
</launch>
