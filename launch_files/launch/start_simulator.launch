<?xml version="1.0"?>
<launch>
    <include file="$(find launch_files)/launch/software_components.launch" pass_all_args="true"/> 
    <include file="$(find simulator_motor_driver)/launch/simulator.launch" pass_all_args="true"/>

    <!--<node name="imu_raw_data_republisher" pkg="topic_tools" type="relay" args="/rover/imu/data /imu/data_raw"/>-->
    <node name="magnetometer_republisher" pkg="simulator_motor_driver" type="magnetometer_republisher.py">
        <remap from="~vect3_msg" to="/rover/magnetometer"/>
        <remap from="~mag_msg" to="/imu/mag"/>
        <remap from="~imu_msg_in" to="/rover/imu"/>
        <remap from="~imu_msg_out" to="/imu/data_raw"/>
    </node>

    <node name="camera_transform_publisher" pkg="tf" type="static_transform_publisher" args="0 0 0.58 0 0 -1.57 map camera_link 10" />
    <!-- <node name="rviz_sim" pkg="rviz" type="rviz" output="screen" /> -->
</launch>
