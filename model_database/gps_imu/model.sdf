<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="gps_imu">
    <link name="gps_imu_link">

      <inertial>
        <mass>0.05</mass>
      </inertial>
      <visual name="visual">
        <geometry>
          <box>
            <size>0.02 0.04 0.01</size>
          </box>
        </geometry>
        <material>
          <lighting>1</lighting>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Blue</name>
          </script>
          <shader type='vertex'>
            <normal_map>__default__</normal_map>
          </shader>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.02 0.04 0.01</size>
          </box>
        </geometry>
      </collision>

      <sensor name="imu" type="imu">
        <always_on>true</always_on>
          <update_rate>100</update_rate>
          <visualize>true</visualize>
          <topic>__default_topic__</topic>
          <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
            <topicName>/odom/imu</topicName>
            <bodyName>gps_imu_link</bodyName>
            <updateRateHZ>100.0</updateRateHZ>
            <gaussianNoise>0.0</gaussianNoise>
            <xyzOffset>0 0 0</xyzOffset>
            <rpyOffset>0 0 0</rpyOffset>
            <frameName>imu_link</frameName>
          </plugin>
          <pose>0 0 0 0 0 0</pose>
          <imu>
          <angular_velocity>
            <x>
              <noise type="gaussian">
                <mean>-0.01</mean>
                <stddev>0.025</stddev>
                <bias_mean>0.0</bias_mean>
                <bias_stddev>0.0</bias_stddev>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0.01</mean>
                <stddev>0.025</stddev>
                <bias_mean>0.0</bias_mean>
                <bias_stddev>0.0</bias_stddev>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>0.01</mean>
                <stddev>0.025</stddev>
                <bias_mean>0.0</bias_mean>
                <bias_stddev>0.0</bias_stddev>
              </noise>
            </z>
          </angular_velocity>
          <linear_acceleration>
            <x>
              <noise type="gaussian">
                <mean>-0.01</mean>
                <stddev>0.025</stddev>
                <bias_mean>0.0</bias_mean>
                <bias_stddev>0.0</bias_stddev>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0.01</mean>
                <stddev>0.025</stddev>
                <bias_mean>0.0</bias_mean>
                <bias_stddev>0.0</bias_stddev>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>-0.01</mean>
                <stddev>0.025</stddev>
                <bias_mean>0.0</bias_mean>
                <bias_stddev>0.0</bias_stddev>
              </noise>
            </z>
          </linear_acceleration>
        </imu>
        <always_on>1</always_on>
        <update_rate>200</update_rate>
      </sensor>

      <!--<sensor name="a_gps_sensor" type="gps">
        <plugin name="custom_gps_sim_2" filename="libSimGpsPlugin.so"/>
      </sensor>-->

      <!--<sensor name="gps_sensor" type="gps">
        <always_on>true</always_on>
        <update_rate>10</update_rate>
        <visualize>true</visualize>
        <topic>rover_gps</topic>
        <plugin name="custom_gps_sim" filename="libSimGpsPlugin.so"/>

        <gps>
          <position_sensing>
            <horizontal>
              <noise type="none">
              </noise>
            </horizontal>
            <vertical>
              <noise type="none">
              </noise>
            </vertical>
          </position_sensing>
          <velocity_sensing>
            <horizontal>
              <noise type="none">
              </noise>
            </horizontal>
            <vertical>
              <noise type="none">
              </noise>
            </vertical>
          </velocity_sensing>
        </gps>
        <always_on>1</always_on>
        <update_rate>10</update_rate>
      </sensor> -->

    </link>
    <plugin name="custom_gps_sim" filename="libhector_gazebo_ros_gps.so">
        <alwaysOn>1</alwaysOn>
        <updateRate>10.0</updateRate>
        <bodyName>gps_imu_link</bodyName>
        <topicName>/odom/fix</topicName>
        <velocityTopicName>fix_velocity</velocityTopicName>
        <drift>2.0 2.0 2.0</drift>
        <gaussianNoise>0.25 0.25 0.25</gaussianNoise>
        <velocityDrift>1 1 1</velocityDrift>
        <velocityGaussianNoise>0.1 0.1 0.1</velocityGaussianNoise>
    </plugin>
  </model>
</sdf>
