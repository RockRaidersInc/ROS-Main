<sdf version='1.6'>
  <world name='sim'>
    <gravity>0 0 -9.8</gravity>
    <magnetic_field>6e-06 2.3e-05 -4.2e-05</magnetic_field>
    <atmosphere type='adiabatic'/>
    <physics name='default_physics' default='0' type='ode'>
      <max_step_size>0.0025</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>250</real_time_update_rate>
    </physics>
    <scene>
      <ambient>0.6 0.6 0.6 1</ambient>
      <background>0.7 0.7 0.7 1</background>
      <shadows>1</shadows>
    </scene>
    <!-- this doesn't actually do anything since we're using the hector GPS plugin in -->
    <spherical_coordinates>
      <surface_model>EARTH_WGS84</surface_model>
      <latitude_deg> 042.729306 </latitude_deg>
      <longitude_deg> -73.680906 </longitude_deg>
      <elevation>0</elevation>
      <heading_deg>0</heading_deg>
    </spherical_coordinates>


<!-- gazebo 7 doesn't seem to support changing the default camera location, this should be uncommented if we move to gazebo 9+ -->
<!--
    <gui>
      <camera>
        <pose> -80 -7 7 0 -0.8 1.57</pose>
      </camera>
    </gui>
-->

    <include>
      <uri>model://sun</uri>
      <pose>0 0 100   0 0 0</pose>
    </include>

    <include>
      <uri>model://Robot_Base</uri>
      <name>rover</name>
      <pose> -76 0 0.5   0 0 0</pose>
    </include>
    <!-- <plugin name="SimMotorNode" filename="libSimMotorNode.so"/> -->


    <include>
      <uri>model://terrain</uri>
      <name>terrain</name>
      <pose> 0 0 0   0 0 0 </pose>
    </include>

    <model name='ground_plane'>
      <static>1</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.25</mu>
                <mu2>0.25</mu2>
              </ode>
              <torsional>
                <ode/>
              </torsional>
            </friction>
            <contact>
              <ode/>
            </contact>
            <bounce/>
          </surface>
          <max_contacts>10</max_contacts>
        </collision>
        <visual name='visual'>
          <cast_shadows>0</cast_shadows>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/White</name>
            </script>
          </material>
        </visual>
        <self_collide>0</self_collide>
        <kinematic>0</kinematic>
        <gravity>1</gravity>
      </link>
      <pose frame=''>-70 0 0 0 -0 0</pose>
    </model>

    <plugin name='FixFrictionNode' filename='libFixFrictionNode.so'/>

  </world>
</sdf>
